	<head>
		<css src="Webfitters_Hero::css/hero.css" />
	</head>
    <body>
    	<referenceContainer name="page.top">
        	<block class="Webfitters\Wholesale\Block\Type" template="Webfitters_Hero::hero.phtml" name="hero" after="navigation.sections" />
